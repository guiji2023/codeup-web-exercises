function isPangrame(sentence) {
  let baseStr = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNPQRSTUVWXYZ";
  for (let i = 0; i < 26; i++) {
    if (!sentence.includes(baseStr.charAt(i))) {
      return false;
    }
  }

  return true;
}

let str = "abcdefgjhijklmnopqrstuvwxyz";

console.log(isPangrame(str));
